<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="symfony\Flat" table="flat">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="flat_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="roomNo" type="smallint" column="room_no" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="area" type="integer" column="area" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="description" type="text" column="description" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="slotNo" type="smallint" column="slot_no" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="adress" type="text" column="adress" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="text" column="Name" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
